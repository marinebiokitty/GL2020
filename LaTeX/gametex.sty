\def\@testfields{%
  \ifx\@notowner\empty%
    \def\@i##1##2{%
      \unless\ifx##1\empty%
        \owner\def\@i####1####2{}%
      \fi}%
    \def\@ii##1##2{}%
    \@ownedfields%
    \def\@i##1##2{}%
    \def\@ii##1##2{%
      \unless\ifx##1\empty%
        \ifx\@owner\empty\suite\fi%
        \owner\def\@ii####1####2{}%
      \fi}%
    \@ownedfields%
    \def\@ii{\@i}%
  \fi}


